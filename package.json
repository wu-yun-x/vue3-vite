{
    "name": "vite-vue3",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "commit:comment": "引导设置规范化的提交信息",
        "commit": "git-cz",
        "prepare": "husky install",
        "lint": "eslint src",
        "fix": "eslint src --fix"
